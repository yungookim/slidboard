<s:TagVisualization x:Class="slidboard.SlidboardView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="http://schemas.microsoft.com/surface/2008"
    Loaded="ObjectVisualization_Loaded"
    >
    <Grid Width="175"
          Height="auto">
        <Ellipse 
            Name="objectWrapper"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            Width="350" 
            Height="400"
            Fill="Transparent" 
            StrokeThickness="20" 
            Stroke="#FF3BFFFF"/>
        <Label Name="ObjectModel"
               Content="Name" 
               HorizontalAlignment="Center"
               VerticalAlignment="Top"
               Height="25" Margin="30,50,0,0" />
    </Grid>
</s:TagVisualization>